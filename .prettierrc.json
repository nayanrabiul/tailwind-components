{
  "trailingComma": "es5",
  "tabWidth": 4,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 100,
  "arrowParens": "always",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": [
        "app/**/*",
        "components/**/*",
        "context/**/*"
      ]
    }
  ],
  "plugins": [
    "prettier-plugin-tailwindcss"
  ]
}
